#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x563e40ced960 .scope module, "program_counter_tb" "program_counter_tb" 2 1;
 .timescale 0 0;
v0x563e40d05660_0 .var "clk", 0 0;
v0x563e40d05720_0 .var "next_pc", 31 0;
v0x563e40d057f0_0 .net "pc_out", 31 0, v0x563e40d05300_0;  1 drivers
v0x563e40d058f0_0 .var "pc_write", 0 0;
v0x563e40d059c0_0 .var "rst_n", 0 0;
E_0x563e40cec610 .event posedge, v0x563e40cb3470_0;
S_0x563e40cedb10 .scope module, "pc_inst" "program_counter" 2 10, 3 1 0, S_0x563e40ced960;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "pc_write";
    .port_info 3 /INPUT 32 "next_pc";
    .port_info 4 /OUTPUT 32 "pc_out";
v0x563e40cb3470_0 .net "clk", 0 0, v0x563e40d05660_0;  1 drivers
v0x563e40d05220_0 .net "next_pc", 31 0, v0x563e40d05720_0;  1 drivers
v0x563e40d05300_0 .var "pc_out", 31 0;
v0x563e40d053f0_0 .net "pc_write", 0 0, v0x563e40d058f0_0;  1 drivers
v0x563e40d054b0_0 .net "rst_n", 0 0, v0x563e40d059c0_0;  1 drivers
E_0x563e40cb3c40/0 .event negedge, v0x563e40d054b0_0;
E_0x563e40cb3c40/1 .event posedge, v0x563e40cb3470_0;
E_0x563e40cb3c40 .event/or E_0x563e40cb3c40/0, E_0x563e40cb3c40/1;
    .scope S_0x563e40cedb10;
T_0 ;
    %wait E_0x563e40cb3c40;
    %load/vec4 v0x563e40d054b0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x563e40d05300_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563e40d053f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x563e40d05220_0;
    %assign/vec4 v0x563e40d05300_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563e40ced960;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x563e40d05660_0;
    %inv;
    %store/vec4 v0x563e40d05660_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x563e40ced960;
T_2 ;
    %vpi_call 2 26 "$dumpfile", "program_counter.vcd" {0 0 0};
    %vpi_call 2 27 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563e40ced960 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e40d05660_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e40d059c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e40d058f0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x563e40d05720_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e40d059c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e40d059c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e40d058f0_0, 0, 1;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x563e40d05720_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x563e40d05720_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 12, 0, 32;
    %store/vec4 v0x563e40d05720_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 4096, 0, 32;
    %store/vec4 v0x563e40d05720_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e40d058f0_0, 0, 1;
    %pushi/vec4 4100, 0, 32;
    %store/vec4 v0x563e40d05720_0, 0, 32;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e40d058f0_0, 0, 1;
    %delay 10, 0;
    %delay 10, 0;
    %vpi_call 2 64 "$display", "All tests completed!" {0 0 0};
    %vpi_call 2 65 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x563e40ced960;
T_3 ;
    %wait E_0x563e40cec610;
    %vpi_call 2 70 "$display", "Time=%0t rst_n=%0b pc_write=%0b next_pc=%0h pc_out=%0h", $time, v0x563e40d059c0_0, v0x563e40d058f0_0, v0x563e40d05720_0, v0x563e40d057f0_0 {0 0 0};
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench/program_counter_tb.v";
